<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

    <package name="cw" extends="cwInterceptor" namespace="/cw">
		<!-- 一个包下可以放这个模块下的所有action -->
		<action name="cwindex" class="com.yq.admin.cw.action.CwSearchVipLogAction" >
		   <result name="success" >/cw/searchviplog.jsp</result>
		</action>
		
		<action name="cjblog" class="com.yq.admin.cw.action.CwSearchVipLogAction" method="vipcjbLog">
		   <result name="success" >/cw/searchvipczblog.jsp</result>
		</action>
		<action name="czbofday" class="com.yq.admin.cw.action.CwSearchVipLogAction" method="czbofday">
		   <result name="success" >/cw/searchvipczblogofday.jsp</result>
		</action>
		<action name="czbofdayexcel" class="com.yq.admin.cw.action.CwSearchVipLogAction" method="czbofdayexcel">
		</action>
		
		<action name="dcdata" class="com.yq.admin.cw.action.VipOutExcelAction">
		   <result name="success" >/cw/dcdata.jsp</result>
		</action>
		
		<action name="outExcelForDatepay" class="com.yq.admin.cw.action.VipOutExcelAction" method="outExcelForDatePay">
		</action>
		
		<action name="outExcelForCjb" class="com.yq.admin.cw.action.VipOutExcelAction" method="outExcelForVipCjb">
		</action>
		<!-- 一币日志收付明细 -->
		<action name="dayofyb" class="com.yq.admin.cw.action.CwSearchVipDayOfYbAction" method="dayofyb">
		   <result name="success" >/cw/searchvipdayofyb.jsp</result>
		</action>
		<action name="dayofyboutexcel" class="com.yq.admin.cw.action.CwSearchVipDayOfYbAction" method="dayofyboutexcel">
		</action>
		<action name="ybservicefee" class="com.yq.admin.cw.action.CwSearchVipDayOfYbAction" method="ybservicefee">
			<result name="success" >/cw/searchvipybservicefee.jsp</result>
		</action>
		<action name="yibiservicerfeeExcel" class="com.yq.admin.cw.action.CwSearchVipDayOfYbAction" method="yibiservicerfeeExcel">
		</action>
		
		
		<!-- 备用保单币收付明细 -->
		<action name="bdblog" class="com.yq.admin.cw.action.CWBdblogAction">
			<result name="success">/cw/searchvipbdblog.jsp</result>
		</action>
		<action name="searchbdblog" class="com.yq.admin.cw.action.CWBdblogAction" method="searchbdblog">
			<result name="success">/cw/searchvipbdblog.jsp</result>
		</action>
		<action name="bdboutExcel" class="com.yq.admin.cw.action.CWBdblogAction" method="bdboutExcel">
		</action>
		
		
		
	</package>

	<!-- 一个包可以就是一个模块 -->
	<package name="cwlogin" extends="noLoginInterceptor" namespace="/">
		<!-- 一个包下可以放这个模块下的所有action -->
		<action name="cwlogin" class="com.yq.admin.cw.action.CwLoginAction">
			<result name="success" >/cw/default.jsp</result>
			<result name="redirect" type="redirect">/cw/cwindex</result>
		</action>
	</package>

   <package name="cwInterceptor" extends="someInterceptor">
		<interceptors>
			<!-- 认证拦截器-->
			<interceptor name="cwInterceptor"
				class="com.yq.admin.cw.interceptor.CwInterceptor" />
			<interceptor-stack name="cwStack">
				<interceptor-ref name="cwInterceptor" />
				<interceptor-ref name="someStack" />
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="cwStack" />
			<global-results>
			<!-- 当没有登录的时候就会跳到首页要求其进行登录 -->
			<result name="nologin" type="redirect">
				/cwlogin
			</result>
		</global-results>
	</package>
</struts>
